[Unit]
Description=Dockerized svcrm
After=docker.service
Requires=docker.service
[Service]
Type=oneshot
RemainAfterExit=yes
WorkingDirectory= /svcrm
ExecStart=/usr/local/bin/docker-compose -f /svcrm/docker-compose.yml up -d
ExecStop=/usr/local/bin/docker-compose -f /svcrm/docker-compose.yml down
[Install]
WantedBy=multi-user.target